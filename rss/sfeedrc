sfeedpath="$PWD/rss/raw"

feeds() {
	for f in $(find . -name "APKBUILD"); do
		. "$f"
		case "$url" in
		https://github.com/*)
			feed "$pkgname" "$url/commits/master.atom"
			feed "$pkgname" "$url/commits/main.atom"
			;;
		https://git.sr.ht/*)
			feed "$pkgname" "$url/log/master/rss.xml"
			feed "$pkgname" "$url/log/main/rss.xml"
			;;
		esac
	done
}
